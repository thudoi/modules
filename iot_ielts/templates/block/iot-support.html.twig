<!-- Add you custom twig html here -->
<!--section-->
{% if nodes %}
  <section class="section section-support">
    <div class="container">
      <div class="row">
        {% for node in nodes %}
          <div class="col-md-4 col-sm-12">
            <div class="sp-icon">
            </div>
            <h3>{{ node.title.value }}</h3>
            {{ node.body.value|raw }}
          </div>
        {% endfor %}
      </div>
    </div>
  </section>
{% endif %}